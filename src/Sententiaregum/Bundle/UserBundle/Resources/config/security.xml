<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="sen.user.service.security.user_provider" class="Sententiaregum\Bundle\UserBundle\Security\UserProvider">
            <argument type="service" id="sen.user.repository" />
        </service>

        <service id="sen.user.service.security.api_key_authenticator" class="Sententiaregum\Bundle\UserBundle\Security\ApiKeyAuthenticator" />
    </services>

</container>
